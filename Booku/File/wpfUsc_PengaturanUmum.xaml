<UserControl x:Class="wpfUsc_PengaturanUmum"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Booku"
             mc:Ignorable="d"
             MinWidth="600"
             MinHeight="400"
             >

    <UserControl.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="../WPF\Styles\StyleAplikasi.xaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </UserControl.Resources>
<Grid>
        <StackPanel VerticalAlignment="Center" HorizontalAlignment="Center">
            <TextBlock Text="Pengaturan Umum"
                       FontSize="18"
                       FontWeight="SemiBold"
                       Foreground="{StaticResource clrTeksSekunder}"
                       HorizontalAlignment="Center"
                       Margin="0,0,0,10"/>
            <TextBlock Text="(Belum ada pengaturan yang tersedia)"
                       FontSize="12"
                       Foreground="{StaticResource clrTeksDisabled}"
                       HorizontalAlignment="Center"/>
        </StackPanel>
    </Grid>

</UserControl>


